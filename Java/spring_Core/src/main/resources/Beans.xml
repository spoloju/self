<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd	
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean class="com.beans.Employee" id="emp">
		<property name="empId" value="1234"></property>
		<property name="empName" value="Shravan"></property>
		<property name="mangerId" value="777"></property>
	</bean>
	<bean class="com.beans.Employee" id="emp2">
		<constructor-arg value="4541"> </constructor-arg>
		<constructor-arg value="Babu"> </constructor-arg>
		<constructor-arg value="8554"> </constructor-arg>
	</bean>
</beans>