package basics;

import java.math.BigDecimal;

public class DefaultValuesDemo {
	
	//static String course ="java";
	static String defaultstring;
	static long defaultlong;
	static char defaultchar;
	static double defaultdouble;
	static int defaultint;
	static float defaultfloat;
	static boolean defaultboolean;
	static BigDecimal shraa;
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//System.out.println(course);
		System.out.println("Default Value for String " + defaultstring);
		System.out.println("Default Value for Long " + defaultlong);
		System.out.println("Default Value for Char-" + defaultchar+"-");
		System.out.println("Default Value for defaultdouble " + defaultdouble);
		System.out.println("Default Value for defaultint " + defaultint);
		System.out.println("Default Value for defaultfloat " + defaultfloat);
		System.out.println("Default Value for defaultboolean " + defaultboolean);
		System.out.println("Default Value for default " + shraa);
		
	}

}
